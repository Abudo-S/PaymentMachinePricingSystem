{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "DayRateLoadBalancer": {
    "Routes": {
      "DayRateRoute": {
        "ClusterId": "DayRateCluster",
        "Match": {
          "Path": "/HttpAPI/MicroservicesProtos/DayRate/{*Method}"
        }
      }
    },
    "Clusters": {
      "DayRateCluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "DayRateCluster/destination1": {
            "Address": "http://localhost:5000/"
          },
          "DayRateCluster/destination2": {
            "Address": "https://localhost:6000/"
          }
        }
      }
    }
  }
}
