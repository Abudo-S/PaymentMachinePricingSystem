{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
    //"EndPoints": {
    //  "Http": {
    //    "Url": "http://localhost:5000"
    //  },
    //  "Https": {
    //    "Url": "https://localhost:5001"
    //  }
    //}
  },
  "DayRateLoadBalancer": {
    "Routes": {
      "DayRateRoute": {
        "ClusterId": "DayRateCluster",
        "Match": {
          "Path": "/HttpAPI/MicroservicesProtos/DayRate/{*Method}"
        }
      }
    },
    "Clusters": {
      "DayRateCluster": {
        "LoadBalancingPolicy": "RoundRobin",
        //"HttpRequest": { //grpc http/2
        //  "Version": "2",
        //  "VersionPolicy": "RequestVersionExact"
        //},
        "Destinations": {
          "DayRateCluster/destination1": {
            "Address": "http://localhost:5000/"
          },
          "DayRateCluster/destination2": {
            "Address": "http://localhost:6000/"
          }
        }
      }
    }
  }
}
